<p id="notice"><%= notice %></p>

<h1 align="center">Product Search Models</h1>

<table align="center" class="table table-hover">
  <thead>
    <tr>
      <th>Query</th>
      <!-- <th>Result</th> -->
      <th>Pag count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_search_models.each do |product_search_model| %>
      <tr>
        <td><%= product_search_model.query %></td>
        <!-- <td><%= product_search_model.result %></td> -->
        <td><%= product_search_model.pag_count %></td>
        <td><%= link_to 'Show', product_search_model %></td>
        <td><%= link_to 'Edit', edit_product_search_model_path(product_search_model) %></td>
        <td><%= link_to 'Destroy', product_search_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Search Model', new_product_search_model_path %>
